{
  "query": {
    "bool": {
      "must": {
        "term": {
          {{ range $key, $value := .Data }}
             "{{ $key }}": "{{ $value }}"
             {{ if (gt (len $.Data) 1) }}
                 ,
             {{ end }}
          {{ end }}
        }
      }
    }
  }

  {{ if (gt $.Size 0) }}
  ,
  {{ end }}
  {{ if (gt $.From 0) }}
  "from": {{.From}}
  {{ end }}
  {{ if (gt $.Size 0) }}
  ,
  {{ end }}
  {{ if (gt $.Size 0) }}
  "size": {{.Size}}
  {{ end }}
}